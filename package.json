        "chart.js": "^2.7.2",
        "jquery": "^3.3.1",
        "laravel-elixir": "^5.0.0",
        "material-icons": "^0.2.3",
        "normalize.css": "^8.0.0",
        "pick-a-color": "^1.2.3",
        "popper.js": "^1.14.4",
        "spectrum": "^1.0.1",
        "typeface-arbutus-slab": "0.0.54"
    }
